{
  "name": "node-rdkafka",
  "version": "v3.6.0",
  "description": "Node.js bindings for librdkafka",
  "librdkafka": "2.12.0",
  "main": "lib/index.js",
  "scripts": {
    "configure": "node-gyp configure",
    "build": "node-gyp build",
    "test": "make test",
    "install": "node-gyp-build",
    "prepack": "node ./ci/prepublish.js",
    "prebuild": "prebuildify --napi --strip",
    "prebuild-all": "prebuildify --napi --strip --arch x64 --arch arm64"
  },
  "binary": {
    "napi_versions": [3, 4, 5, 6, 7, 8, 9]
  },
  "keywords": [
    "kafka",
    "librdkafka"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Blizzard/node-rdkafka.git"
  },
  "contributors": [
    {
      "name": "Stephen Parente",
      "email": "webmakersteve@gmail.com"
    },
    {
      "name": "Matt Gollob",
      "email": "mattness@users.noreply.github.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "bluebird": "^3.5.3",
    "jsdoc": "^4.0.2",
    "jshint": "^2.10.1",
    "mocha": "^10.2.0",
    "node-gyp": "^11.1.0",
    "prebuildify": "^6.0.1",
    "toolkit-jsdoc": "^1.0.0",
    "bindings": "^1.3.1"
  },
  "dependencies": {
    "nan": "^2.22.0",
    "node-gyp-build": "^4.8.4"
  },
  "engines": {
    "node": ">=16 <25"
  }
}
